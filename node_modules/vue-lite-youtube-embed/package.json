{
  "name": "vue-lite-youtube-embed",
  "type": "module",
  "version": "1.2.0",
  "description": "A private by default, faster and cleaner YouTube embed component for Vue applications.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wobsoriano/vue-lite-youtube-embed.git"
  },
  "keywords": [
    "vue",
    "youtube",
    "embed"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./style.css": "./dist/index.css"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint .",
    "release": "bumpp && npm publish",
    "update-deps": "taze -w && pnpm i",
    "build": "tsup",
    "dev": "vite serve dev",
    "dev:build": "vue-tsc && vite build dev",
    "dev:preview": "vite preview dev",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.7.1",
    "vue": "^2.6.14 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "dependencies": {
    "vue-demi": "latest"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.34.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "bumpp": "^8.2.1",
    "eslint": "^8.31.0",
    "taze": "^0.8.5",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vue": "^3.2.45",
    "vue-tsc": "^1.0.24"
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
}
